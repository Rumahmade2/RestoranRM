{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "color-adjust",
          "-ms-touch-action"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}